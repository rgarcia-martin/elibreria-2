{
  "name": "app/gestion",
  "description": "Proyecto Symfony 7.3 (PHP 8.2) con arquitectura hexagonal / DDD",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^8.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/doctrine-bundle": "^2.12",
    "doctrine/doctrine-migrations-bundle": "*",
    "doctrine/migrations": "^3.7",
    "doctrine/orm": "^3.2",
    "ramsey/uuid": "^4.7",
    "symfony/console": "^7.3",
    "symfony/dotenv": "^7.3",
    "symfony/flex": "^2.4",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-client": "^7.3",
    "symfony/messenger": "^7.3",
    "symfony/monolog-bundle": "^3.8",
    "symfony/property-access": "^7.3",
    "symfony/property-info": "^7.3",
    "symfony/runtime": "^7.3",
    "symfony/serializer": "^7.3",
    "symfony/uid": "^7.3",
    "symfony/validator": "^7.3",
    "symfony/yaml": "^7.3"
  },
  "require-dev": {
    "symfony/var-dumper": "^7.3",
    "symfony/debug-bundle": "^7.3",
    "symfony/maker-bundle": "^1.60",
    "symfony/phpunit-bridge": "^7.3",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": { "*": "dist" },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "7.3.*"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
